{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "africastalking/africastalking": "^3.0",
        "barryvdh/laravel-dompdf": "*",
        "beyondcode/laravel-comments": "^1.2",
        "bigbharatjain/laravel-clickatell": "^1.0",
        "cloudinary-labs/cloudinary-laravel": "^1.0",
        "doctrine/dbal": "^3.3",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "haruncpi/laravel-simple-filemanager": "^1.0",
        "ixudra/curl": "^6.21",
        "ladumor/laravel-pwa": "^0.0.3",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^1.6",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "macsidigital/laravel-zoom": "^4.1",
        "mavinoo/laravel-batch": "^2.2",
        "mckenziearts/laravel-notify": "^2.1",
        "messagebird/php-rest-api": "^1.19",
        "nascent-africa/jetstrap": "^1.4",
        "okipa/laravel-table": "^4.0",
        "php-flasher/flasher": "^1.7",
        "santigarcor/laratrust": "^6.3",
        "seshac/otp-generator": "^0.0.6",
        "silviolleite/laravelpwa": "^2.0",
        "socialiteproviders/yahoo": "^4.1",
        "spatie/emoji": "^2.2",
        "spatie/laravel-google-calendar": "^3.2",
        "stancl/tenancy": "^3.4",
        "unisharp/laravel-filemanager": "^2.2",
        "yajra/laravel-datatables-oracle": "~9.0",
        "yajra/laravel-disqus": "^1.2"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nascent-africa/jetstrap": "^1.4",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan cache:clear",
            "@php artisan config:cache",
            "@php artisan config:clear",
            "@php artisan optimize:clear",

            
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
